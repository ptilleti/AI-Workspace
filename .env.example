# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# OpenAI API Key (get from: https://platform.openai.com/api-keys)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Use local models instead (no API key needed)
# Set to 'ollama' to use local models, 'openai' for OpenAI API
LLM_PROVIDER=openai

# Model configuration
OPENAI_MODEL=gpt-4o-mini  # Cheaper and faster; use gpt-4o for better quality
TEMPERATURE=0.7            # Creativity level (0.0-1.0, lower = more focused)
